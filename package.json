{
  "name": "tabsalad",
  "version": "2.1.3",
  "description": "Paste cells from your spreadsheet and convert them into HTML table markup.",
  "main": "src/scripts/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AshKyd/tabsalad"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "node_modules/browserify/bin/cmd.js src/scripts/index.js>dist/index.js & node_modules/browserify/bin/cmd.js src/worker/worker.js>dist/worker.js",
    "build-css": "node_modules/less/bin/lessc src/css/style.less > dist/style.css",
    "build-misc": "cp src/*.html dist/",
    "clean": "rm -rf dist;mkdir dist",
    "build": "npm run clean; npm run build-js & npm run build-css & npm run build-misc;wait"
  },
  "author": "Ash Kyd <ash@kyd.com.au>",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^3.2.0",
    "brfs": "~1.4.0",
    "browserify": "^5.12.0",
    "clean-css": "^2.2.16",
    "del": "~1.2.0",
    "jquery": "^2.1.1",
    "less": "^2.7.1",
    "mustache": "^2.0.0",
    "serviceworker-cache-polyfill": "~3.0.0",
    "through": "~2.3.7"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "dependencies": {}
}
