{
  "name": "tabsalad",
  "version": "2.1.3",
  "description": "Paste cells from your spreadsheet and convert them into HTML table markup.",
  "main": "src/scripts/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AshKyd/tabsalad"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite src",
    "dev:web": "vite web",
    "build:web": "vite build web --outDir ../dist",
    "build:app": "vite build src --base /app/ --outDir ../dist/app",
    "build": "rm -rf dist && npm run build:web && npm run build:app",
    "start": "npm run dev"
  },
  "author": "Ash Kyd <ash@kyd.com.au>",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^3.4.1",
    "jquery": "^3.3.1",
    "less": "^3.9.0",
    "mustache": "^3.0.1",
    "vite": "^6.4.1"
  }
}
